# ~/dotfiles/aws/config
# AWS CLI設定ファイル
# 認証情報は1Password CLIで管理されます

# デフォルトプロファイル
[default]
region = ap-northeast-1
output = json
credential_process = AWS_PROFILE=default ~/.aws/get-credentials.sh

# Wonder-Screen-Dev_copilot-user プロファイル
[profile Wonder-Screen-Dev_copilot-user]
region = ap-northeast-1
output = json
credential_process = AWS_PROFILE=Wonder-Screen-Dev_copilot-user ~/.aws/get-credentials.sh

# 他のプロファイルを追加する場合は、以下の形式で追加してください：
# [profile プロファイル名]
# region = ap-northeast-1
# output = json
# credential_process = AWS_PROFILE=プロファイル名 ~/.aws/get-credentials.sh

